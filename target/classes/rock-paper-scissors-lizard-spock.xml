<?xml version="1.0" encoding="ISO-8859-1"?>
<game>

    <weapon name="rock"/>
    <weapon name="scissors"/>
    <weapon name="paper"/>
    <weapon name="lizard"/>
    <weapon name="spock"/>

    <rule weapon="rock">
        <beats name="crushes" weapon="scissors"/>
        <beats name="crushes" weapon="lizard"/>
    </rule>
    <rule weapon="paper">
        <beats name="covers" weapon="rock"/>
        <beats name="disproves" weapon="spock"/>
    </rule>
    <rule weapon="scissors">
        <beats name="cuts" weapon="paper"/>
        <beats name="decapitates" weapon="lizard"/>
    </rule>
    <rule weapon="lizard">
        <beats name="poisons" weapon="spock"/>
        <beats name="eats" weapon="paper"/>
    </rule>
    <rule weapon="spock">
        <beats name="smashes" weapon="scissors"/>
        <beats name="vaporizes" weapon="rock"/>
    </rule>

</game>